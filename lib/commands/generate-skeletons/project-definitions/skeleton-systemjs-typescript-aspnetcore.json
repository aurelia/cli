{
  "name": "skeleton-systemjs-typescript-aspnetcore",
  "type": "project:application",
  "bundler": {
    "id": "cli",
    "displayName": "Aurelia-CLI"
  },
  "platform": {
    "id": "aspnetcore",
    "displayName": "ASP.NET Core"
  },
  "loader": {
    "id": "system",
    "displayName": "SystemJS"
  },
  "transpiler": {
    "id": "typescript",
    "displayName": "TypeScript",
    "fileExtension": ".ts"
  },
  "markupProcessor": {
    "id": "minimum",
    "displayName": "Minimal Minification",
    "fileExtension": ".html"
  },
  "cssProcessor": {
    "id": "none",
    "displayName": "None",
    "fileExtension": ".css"
  },
  "editor": {
    "id": "vscode",
    "displayName": "Visual Studio Code"
  },
  "unitTestRunners": [
    {
      "id": "jest",
      "displayName": "Jest"
    },
    {
      "id": "karma",
      "displayName": "Karma",
      "source": "test/unit/**/*.ts"
    }
  ],
  "integrationTestRunner": {
    "id": "protractor",
    "displayName": "Protractor"
  }
}